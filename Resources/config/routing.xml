<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="fullcalendar_loadevents" path="/fullcalendar/load">
        <default key="_controller">fullCalendarBundle:Calendar:load</default>
        <option key="expose">true</option>
    </route>
    <route id="fullcalendar_changedate" path="/fullcalendar/change">
        <default key="_controller">fullCalendarBundle:Calendar:change</default>
        <option key="expose">true</option>
    </route>
    <route id="fullcalendar_resizedate" path="/fullcalendar/resize">
        <default key="_controller">fullCalendarBundle:Calendar:resize</default>
        <option key="expose">true</option>
    </route>
    <route id="fullcalendar_delevent" path="/fullcalendar/delete">
        <default key="_controller">fullCalendarBundle:Calendar:delete</default>
        <option key="expose">true</option>
    </route>
    <route id="fullcalendar_addevent" path="/fullcalendar/add">
        <default key="_controller">fullCalendarBundle:Calendar:add</default>
        <option key="expose">true</option>
    </route>
    <route id="fullcalendar_checkevent" path="/fullcalendar/check">
        <default key="_controller">fullCalendarBundle:Calendar:check</default>
        <option key="expose">true</option>
    </route>
    <route id="fullcalendar_getdozentevent" path="/fullcalendar/geteventdozent">
        <default key="_controller">fullCalendarBundle:Calendar:geteventdozent</default>
        <option key="expose">true</option>
    </route>
    <route id="fullcalendar_feirtage" path="/fullcalendar/reloadfeirtage">
        <default key="_controller">AppBundle:feirtage:MigrateFreiTagEvents</default>
        <option key="expose">true</option>
    </route>

</routes>